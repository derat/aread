{
  "manifest_version": 2,
  "name": "aread",
  "version": "5",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "icons": {
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "aread",
    "default_popup": "popup.html"
  }
}
